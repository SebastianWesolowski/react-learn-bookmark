{
  "name": "react-learn-bookmark",
  "description": "Simple React app - React learn -Bookmark ",
  "author": "Sebastian Weso≈Çowski <sebastian@wesolowski.dev>",
  "version": "0.1.0",
  "scripts": {
    "start": "yarn develop",
    "develop": "cross-env GATSBY_GRAPHQL_IDE=playground gatsby develop",
    "serve": "gatsby serve",
    "ssr": "yarn build && yarn serve",
    "build:dir": "mkdirp ./public/icons/",
    "build:favicons": "node scripts/favicons",
    "build": "yarn build:dir && yarn build:favicons && gatsby build",
    "format": "prettier --config ./.prettierrc --write \"**/*.{js,jsx,json,md}\"",
    "analyze": "ANALYZE_BUNDLE_SIZE=true gatsby build"
  },
  "greenkeeper": {
    "ignore": [
      "core-js"
    ]
  },
  "private": true,
  "keywords": [
    "gatsby",
    "React Learn",
    "Bookmark"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/SebastianWesolowski/react-learn-bookmark"
  },
  "bugs": {
    "url": "https://github.com/SebastianWesolowski/react-learn-bookmark/issues"
  },
  "dependencies": {
    "antd": "^3.26.9",
    "cross-env": "^7.0.0",
    "gatsby": "^2.20.21",
    "gatsby-image": "^2.2.41",
    "gatsby-plugin-antd": "^2.1.0",
    "gatsby-plugin-eslint": "^2.0.8",
    "gatsby-plugin-netlify": "^2.1.32",
    "gatsby-plugin-react-helmet": "^3.1.22",
    "gatsby-plugin-react-svg": "^3.0.0",
    "gatsby-plugin-sharp": "^2.4.5",
    "gatsby-plugin-sitemap": "^2.2.27",
    "gatsby-plugin-styled-components": "^3.1.19",
    "gatsby-plugin-web-font-loader": "^1.0.4",
    "gatsby-plugin-webpack-bundle-analyzer": "^1.0.5",
    "gatsby-plugin-webpack-size": "^1.0.0",
    "gatsby-source-filesystem": "^2.1.48",
    "gatsby-transformer-json": "^2.2.26",
    "gatsby-transformer-sharp": "^2.3.14",
    "prop-types": "^15.7.2",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-helmet": "^5.2.1",
    "styled-components": "^5.0.1"
  },
  "devDependencies": {
    "babel-eslint": "^10.0.3",
    "eslint": "^6.8.0",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-config-prettier": "^6.10.0",
    "eslint-config-react-app": "^5.2.0",
    "eslint-loader": "^3.0.3",
    "eslint-plugin-flowtype": "^4.6.0",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-react": "^7.18.3",
    "eslint-plugin-react-hooks": "^2.3.0",
    "favicons": "^6.0.0",
    "husky": "^4.2.2",
    "lint-staged": "^10.0.7",
    "mkdirp": "^1.0.3",
    "prettier": "^1.19.1",
    "stylelint": "11.1.1",
    "stylelint-config-recommended": "^3.0.0",
    "stylelint-config-styled-components": "^0.1.1",
    "stylelint-processor-styled-components": "^1.10.0"
  }
